<Window x:Class="WorkspaceLauncher.Views.ReturnStringView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WorkspaceLauncher.Views"
        mc:Ignorable="d"
        Title="{Binding Title}" Height="200" Width="450" WindowStartupLocation="Manual" WindowStyle="None">
    <Grid>
		<StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
			<TextBlock Text="{Binding Message}"/>
			<TextBox Text="{Binding Value}"/>
			<StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
				<Button Content="Save" Width="75" Margin="10" Click="Button_Click"/>
				<Button Content="Cancel" Width="75" Margin="10" Click="Button_Click_1"/>
			</StackPanel>
		</StackPanel>
    </Grid>
</Window>
